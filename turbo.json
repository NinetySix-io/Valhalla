{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**", ".next/**", "public/dist/**"],
      "dependsOn": ["^build"]
    },
    "thor#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"]
    },
    "codegen": {
      "dependsOn": ["^codegen"],
      "outputs": ["src/generated/**"]
    },
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  },
  "globalDependencies": ["$GITHUB_TOKEN", "tsconfig.json"]
}
