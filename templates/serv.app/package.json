{
  "name": "serv.app",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "dev": "rimraf dist && npm run start:debug",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch --runInBand",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "test:ci": "jest --ci --runInBand",
    "proto:generate": "node ../../scripts/generate.proto"
  },
  "dependencies": {
    "@nestcloud2/boot": "^0.8.3",
    "@nestcloud2/core": "^0.8.3",
    "@nestjs/common": "^8.4.5",
    "@nestjs/core": "^8.4.5",
    "@nestjs/cqrs": "^8.0.4",
    "@nestjs/graphql": "^10.0.11",
    "@nestjs/microservices": "^8.4.4",
    "@nestjs/platform-fastify": "^8.4.4",
    "@nestjs/terminus": "^8.0.6",
    "@typegoose/typegoose": "^9.8.1",
    "@valhalla/serv.clients": "workspace:*",
    "@valhalla/serv.core": "workspace:*",
    "@valhalla/utilities": "workspace:*",
    "casbin": "^5.15.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dayjs": "^1.11.2",
    "dotenv": "^16.0.1",
    "graphql": "^16.5.0",
    "long": "^5.2.0",
    "mongodb": "^4.6.0",
    "mongoose": "^6.3.3",
    "nestjs-casbin": "^2.0.8",
    "nestjs-typegoose": "^7.1.38",
    "protobufjs": "^6.11.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.5",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.4",
    "rimraf": "^3.0.2",
    "ts-proto": "^1.112.1",
    "typescript": "^4.6.4"
  }
}
