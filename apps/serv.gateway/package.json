{
  "name": "serv.gateway",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:debug": "nest start -d -w",
    "start:prod": "node dist/main",
    "dev": "rimraf dist && npm run start:debug",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch --runInBand",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "test:ci": "jest --ci --runInBand"
  },
  "dependencies": {
    "@apollo/composition": "^2.0.3",
    "@apollo/federation-internals": "^2.0.3",
    "@apollo/gateway": "^2.0.2",
    "@nestcloud2/boot": "^0.8.3",
    "@nestcloud2/common": "^0.8.3",
    "@nestcloud2/config": "^0.8.3",
    "@nestcloud2/consul": "^0.8.3",
    "@nestcloud2/service": "^0.8.3",
    "@nestjs/apollo": "^10.0.11",
    "@nestjs/common": "^8.4.5",
    "@nestjs/core": "^8.4.5",
    "@nestjs/cqrs": "^8.0.4",
    "@nestjs/graphql": "^10.0.11",
    "@nestjs/microservices": "^8.4.5",
    "@nestjs/schedule": "^1.1.0",
    "@nestjs/terminus": "^8.0.6",
    "@valhalla/serv.core": "workspace:*",
    "@valhalla/utilities": "workspace:*",
    "apollo-server-types": "^3.6.0",
    "consul": "^0.34.1",
    "cron-time-generator": "^1.3.0",
    "fastify": "^3.29.0",
    "graphql": "^16.5.0",
    "lodash.keyby": "^4.6.0",
    "long": "^5.2.0",
    "node-fetch": "^3.2.6",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.5",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.4",
    "@types/lodash.keyby": "^4.6.7",
    "dotenv": "^16.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  }
}
