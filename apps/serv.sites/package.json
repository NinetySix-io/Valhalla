{
  "name": "serv.sites",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "dev": "rimraf dist && npm run start:debug",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch --runInBand",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "test:ci": "jest --ci --runInBand",
    "codegen": "node ../../scripts/generate.proto"
  },
  "dependencies": {
    "@nestcloud2/boot": "^0.8.5",
    "@nestcloud2/core": "^0.8.5",
    "@nestjs/common": "^8.4.5",
    "@nestjs/core": "^8.4.5",
    "@nestjs/cqrs": "^8.0.4",
    "@nestjs/graphql": "^10.0.11",
    "@nestjs/microservices": "^8.4.4",
    "@nestjs/terminus": "^8.0.6",
    "@valhalla/serv.clients": "workspace:*",
    "@valhalla/serv.core": "workspace:*",
    "@valhalla/utilities": "workspace:*",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.2",
    "graphql": "^16.6.0",
    "graphql-type-json": "^0.3.2",
    "kindagoose": "^1.2.4",
    "lodash.compact": "^3.0.1",
    "lodash.isempty": "^4.4.0",
    "lodash.isnil": "^4.0.0",
    "lodash.keyby": "^4.6.0",
    "lodash.omitby": "^4.6.0",
    "lodash.pick": "^4.4.0",
    "long": "^5.2.0",
    "merge-anything": "^5.0.4",
    "protobufjs": "^6.11.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.5",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.7",
    "@types/jest": "^28.1.4",
    "@types/lodash": "^4.14.186",
    "@types/lodash.compact": "^3.0.7",
    "@types/lodash.isempty": "^4.4.7",
    "@types/lodash.isnil": "^4.0.7",
    "@types/lodash.keyby": "^4.6.7",
    "@types/lodash.omitby": "^4.6.7",
    "@types/lodash.pick": "^4.4.7",
    "jest": "^29.0.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.1",
    "ts-proto": "^1.125.0",
    "typescript": "^4.8.3"
  }
}
